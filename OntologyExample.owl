    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreaciónPrototipo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreaciónPrototipo">
