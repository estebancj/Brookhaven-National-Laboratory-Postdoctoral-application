    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreaciónServicio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreaciónServicio">
